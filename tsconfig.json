{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "module": "CommonJS",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "outDir": "dist",
    "types": [
      "@nuxt/types",
      "@types/node",
      "image-meta",
      "./types"
    ],
    "paths": {
      "~image/*": ["src/runtime/*"]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/runtime"
  ]
}
